{
  "name": "react-2023q1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "lint": "eslint . src/**/*.ts[x]",
    "lint:fix": "eslint --fix",
    "format": "npx prettier --write .",
    "test": "vitest",
    "cy": "cypress open",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:5173 cy:open-e2e",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.3.5",
    "express": "^4.18.2",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
